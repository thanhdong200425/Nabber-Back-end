{
    "name": "nabber-back-end",
    "version": "1.0.0",
    "description": "This is back-end side for Nabber",
    "type": "commonjs",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "ts-node-dev index.ts",
        "test": "echo \"Error: no test specified\" && exit 1",
        "migrate": "tsc && sequelize-cli db:migrate",
        "migrate:undo": "tsc && sequelize-cli db:migrate:undo",
        "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
        "migration:generate": "sequelize-cli migration:generate --name",
        "seed:generate": "sequelize-cli seed:generate --name",
        "seed:all": "tsc && sequelize-cli db:seed:all",
        "seed": "tsc && sequelize-cli db:seed --seed",
        "server": "tsc && nodemon"
    },
    "author": "Thanh Dong",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.1.1",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.11.5",
        "sequelize": "^6.37.3",
        "sequelize-cli": "^6.6.2",
        "sequelize-typescript": "^2.1.6",
        "ts-node-dev": "^2.0.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@types/bcrypt": "^5.0.2",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/node": "^20.12.7",
        "nodemon": "^3.1.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5"
    }
}
